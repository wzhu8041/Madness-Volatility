   "source": [
    "url = 'https://en.wikipedia.org/wiki/NCAA_Division_I_men%27s_basketball_tournament_upsets#National_championship'\n",
    "df = pd.read_html(url)\n",
    "data = np.empty((0,3))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "fe073dc8-56f7-4355-9692-23b9daa7ff2c",
   "metadata": {},
